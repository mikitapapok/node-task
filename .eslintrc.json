{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["prettier", "airbnb-base"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "prettier/prettier": "error",
        "indent": "off",
        "consistent-return": "off",
        "object-curly-newline": "off",
        "no-unused-vars": "off",
        "no-unused-expressions": "off"
    }
}
